{
  "name": "shopify-dawn-starter",
  "version": "1.0.0",
  "description": "Starter for Shopify Dawn theme",
  "main": "Gulpfile.js",
  "scripts": {
    "prepare": "husky install",
    "build": "npx gulp build",
    "dev": "npx gulp default"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/volho-dev/shopify-dawn-starter.git"
  },
  "keywords": [
    "shopify"
  ],
  "author": "volho-dev",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/volho-dev/shopify-dawn-starter/issues"
  },
  "homepage": "https://github.com/volho-dev/shopify-dawn-starter#readme",
  "engines": {
    "npm": ">=9.3.1",
    "node": ">=18.14.0"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "browsers": [
              "last 2 versions"
            ]
          }
        }
      ]
    ]
  },
  "lint-staged": {
    "./layout/*.liquid": "prettier --write",
    "./snippets/*.liquid": "prettier --write",
    "./templates/*.liquid": "prettier --write",
    "./sections/*.liquid": "prettier --write",
    "./src/styles/*.scss": "prettier --write",
    "./src/scripts/*.js": "prettier --write",
    "./templates/*.json": "prettier --write",
    "./locales/*.json": "prettier --write",
    "./config/*.json": "prettier --write",
    "./Gulpfile.js": "prettier --write"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/preset-env": "^7.20.2",
    "@babel/register": "^7.18.9",
    "@shopify/prettier-plugin-liquid": "^1.0.6",
    "babel-preset-env": "^1.7.0",
    "del": "^7.0.0",
    "dotenv": "^16.0.3",
    "git-rev": "^0.2.1",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^8.0.0",
    "gulp-babel": "^8.0.0",
    "gulp-changed": "^4.0.3",
    "gulp-clean-css": "^4.3.0",
    "gulp-cli": "^2.3.0",
    "gulp-concat": "^2.6.1",
    "gulp-filter": "^7.0.0",
    "gulp-flatten": "^0.4.0",
    "gulp-notify": "^4.0.0",
    "gulp-plumber": "^1.2.1",
    "gulp-rename": "^2.0.0",
    "gulp-sass": "^5.1.0",
    "gulp-sourcemaps": "^3.0.0",
    "gulp-uglify": "^3.0.2",
    "gulp-watch": "^5.0.1",
    "gulp-shell": "^0.8.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.2",
    "node-sass": "^8.0.0",
    "open": "^8.4.1",
    "prettier": "^2.8.4",
    "run-sequence": "^2.2.1",
    "sass": "^1.58.1"
  },
  "dependencies": {
    "swiper": "^8.4.7"
  }
}
